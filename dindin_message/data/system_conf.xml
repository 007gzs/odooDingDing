<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

		<!--工作消息管理-->
		<record id="ali_dindin_system_conf_send_work_message" model="ali.dindin.system.conf">
			<field name="name">发送工作通知消息</field>
			<field name="key">send_work_message</field>
			<field name="value">https://oapi.dingtalk.com/topapi/message/corpconversation/asyncsend_v2?access_token=</field>
		</record>

		<record id="ali_dindin_system_conf_search_work_message" model="ali.dindin.system.conf">
			<field name="name">查询工作通知消息的发送进度</field>
			<field name="key">search_work_message</field>
			<field name="value">https://oapi.dingtalk.com/topapi/message/corpconversation/getsendprogress?access_token=</field>
		</record>

		<record id="ali_dindin_system_conf_work_message_result" model="ali.dindin.system.conf">
			<field name="name">查询工作通知消息的发送结果</field>
			<field name="key">work_message_result</field>
			<field name="value">https://oapi.dingtalk.com/topapi/message/corpconversation/getsendresult?access_token=</field>
		</record>


    </data>
</odoo>