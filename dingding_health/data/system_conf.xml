<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="dingding_parameter_get_user_health_status" model="dingding.parameter">
            <field name="name">获取用户钉钉运动开启状态</field>
            <field name="key">get_user_health_status</field>
            <field name="value">https://oapi.dingtalk.com/topapi/health/stepinfo/getuserstatus?access_token=</field>
        </record>

        <record id="dingding_parameter_get_health_list" model="dingding.parameter">
            <field name="name">获取个人或部门的钉钉运动数据</field>
            <field name="key">get_health_list</field>
            <field name="value">https://oapi.dingtalk.com/topapi/health/stepinfo/list?access_token=</field>
        </record>

        <record id="dingding_parameter_hrm_queryonjob" model="dingding.parameter">
            <field name="name">批量获取钉钉运动数据</field>
            <field name="key">get_health_list_by_userid</field>
            <field name="value">https://oapi.dingtalk.com/topapi/health/stepinfo/listbyuserid?access_token=</field>
        </record>

    </data>
</odoo>